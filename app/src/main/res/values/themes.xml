<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.CityEye" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorBlack">@color/black</item>
        <item name="colorWhite">@color/white</item>
        <item name="android:windowContentTransitions">true</item>
    </style>

    <style name="Theme.CityEye" parent="Base.Theme.CityEye" />

    <style name="Theme.CityEye.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.CityEye.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.CityEye.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>